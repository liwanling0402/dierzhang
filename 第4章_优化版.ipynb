    "    axes[i].set_title(f'科赫雪花 (迭代 {order} 次)')\n",
    "    axes[i].set_aspect('equal')\n",
    "    axes[i].axis('off')\n",
    "\n",
    "plt.suptitle('科赫雪花分形图案', fontsize=16, fontweight='bold')\n",
    "plt.tight_layout()\n",
    "plt.show()"
