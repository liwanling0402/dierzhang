    "axes[0, 1].set_title('自定义标记')\n",
    "\n",
    "# 不同标记类型\n",
    "markers = ['o', 's', '^', 'D', 'v']\n",
    "for i, marker in enumerate(markers):\n",
    "    axes[1, 0].plot([i], [i], marker=marker, markersize=10, label=marker)\n",
    "axes[1, 0].legend()\n",
    "axes[1, 0].set_title('常用标记类型')\n",
    "axes[1, 0].set_xlim(-1, 5)\n",
    "axes[1, 0].set_ylim(-1, 5)\n",
    "\n",
    "# 线型与标记组合\n",
    "x = [1, 2, 3, 4]\n",
    "axes[1, 1].plot(x, [2, 3, 1, 4], 'o-', label='圆形标记+实线')\n",
    "axes[1, 1].plot(x, [1, 2, 4, 3], 's--', label='方形标记+虚线')\n",
    "axes[1, 1].plot(x, [3, 1, 2, 1], '^:', label='三角形标记+点线')\n",
    "axes[1, 1].legend()\n",
